<%- include('../../compartido/cabecera')%>
<div class="container">
    <h1>Crear Usuario</h1>
    <form action="https://www.google.com"
          method="GET"
    >
        <div class="form-group">
            <label for="nombreID">Nombre</label>
            <input id="nombreID"
                   type="text"
                   name="nombre"
                   placeholder="Ej: Veronica Olmedo"
                   title="Ingrese nombre de usuario"
                   class="form-control">
            <small id="nombreIDHelp"
                   class="form-text text-muted">
                Ingrese un nombre mayor a 3 caracteres y menor a 10
            </small>
        </div>
        <div class="form-group">
            <label for="cedulaID">Cedula</label>
            <input id="cedulaID"
                   type="text"
                   name="cedula"
                   placeholder="Ej: 1720572773"
                   title="Ingrese la cedula del usuario"
                   class="form-control">
            <small id="cedulaIDHelp"
                   class="form-text text-muted">
                Ingrese una cedula valida
            </small>
        </div>


        <button type="subtmit"
                class="btn btn-success"
        > Crear Usuario</button>
        <button type="reset"
                class="btn btn-secondary"
        > Limpiar formulario</button>
    </form>
</div>

<%- include('../../compartido/pie-de-pagina')%>